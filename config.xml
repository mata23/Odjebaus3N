<?xml version='1.0' encoding='utf-8'?>
<widget id="com.odjebaus3.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Odjebaus 3</name>
    <description>
        Odjebaus v.3.0
    </description>
    <author email="support@tippsoft.com" href="https://www.tippsoft.com">
        TippSoft Team
    </author>
	<preference name="phonegap-version" value="cli-8.0.0" />
    <content src="index.html" />
	<plugin name="cordova-plugin-device" source="npm" spec="~1.1.1" />
	<plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.2" />
	<plugin name="cordova-plugin-splashscreen" source="npm" spec="~3.2.1" />
	<plugin name="phonegap-plugin-push" spec="~2.1.2">
        <variable name="FCM_VERSION" value="11.0.1" />
    </plugin>
	<platform name="android">
        <icon density="ldpi" src="www/icons/1.png" />
        <icon density="mdpi" src="www/icons/2.png" />
        <icon density="hdpi" src="www/icons/3.png" />
        <icon density="xhdpi" src="www/icons/4.png" />
        <icon density="xxhdpi" src="www/icons/5.png" />
        <icon density="xxxhdpi" src="www/icons/6.png" />
        <splash density="land-ldpi" src="www/splash/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="www/splash/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="www/splash/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="www/splash/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="www/splash/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="www/splash/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="www/splash/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="www/splash/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="www/splash/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="www/splash/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="www/splash/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="www/splash/drawable-port-xxxhdpi-screen.png" />
		<allow-intent href="market:*" />
		<resource-file src="google-services.json" target="app/google-services.json" />
		<resource-file src="platforms/browser/res/raw/money.wav" target="app/platforms/browser/res/raw/money.wav" />
		
		
    </platform>
    <access origin="*" />
	<allow-intent href="http://*/*"/>
	<allow-intent href="https://*/*"/>
	<engine name="android" spec="~7.0.0" />
</widget>
